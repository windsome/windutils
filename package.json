{
  "name": "windutils",
  "version": "1.0.0",
  "description": "个人用于js开发所引用的一些工具类的封装,主要避免在每次项目中都手动拷贝进去",
  "keywords": [
    "windsome",
    "utils"
  ],
  "homepage": "https://github.com/windsome/windutils",
  "author": "windsome.feng <36081189@qq.com> (https://github.com/windsome/)",
  "scripts": {
    "build:lib": "rm lib/* && cross-env BABEL_ENV=es babel src/lib --out-dir lib",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "spark-md5": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "prop-types": "^15.6.0"
  },
  "babel": {
    "env": {
      "es": {
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-object-rest-spread",
          "transform-class-properties"
        ]
      }
    }
  }
}
